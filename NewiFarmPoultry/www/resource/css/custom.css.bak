.ios-theme-noline
{
    background: #FFF -webkit-radial-gradient(circle, #FFF, #dee2e4);
}
div.home-icon
{
    width: 114px;
    height: 114px; /*border:1px solid #DBDBDB;*/
    text-align: center;
    display: block;
    float: left;
    margin-left: 15px;
    margin-bottom: 30px;
}
div.home-icon img
{
    width: 114px;
    height: 114px;
}
div.home-icon span
{
    font-size: 18px;
    font-weight: bold;
    display: block;
}

.customer-header
{
    background-color: #F7C942;
    border-bottom: 1px solid #CCC;
    color: #333;
    text-shadow: none;
    font-size: 16px;
}

.product-img
{
    border: 2px solid #DBDBDB;
    width: 57px;
    height: 57px;
    margin-top: 7px;
}
.up-img
{
    float: right;
    width: 20px;
    height: 20px;
    position: absolute;
    right: 5px;
    top: 17px;
    cursor: pointer;
}
.down-img
{
    float: right;
    width: 20px;
    height: 20px;
    position: absolute;
    right: 5px;
    bottom: 17px;
    cursor: pointer;
}


div.freezer
{
    border-radius: 5px;
    border: 3px solid #5E87B0;
    width: 120px;
    height: 100px;
    float: left;
    margin-right: 10px;
}

.freezer img
{
    width: 100%;
    height: 100%;
}


